{
  "name": "babble-buds",
  "version": "0.8.2",
  "description": "An open source version of puppet pals",
  "main": "src/electron-starter.js",
  "homepage": "./",
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build && ./build.sh",
    "test": "node_modules/.bin/mocha -w",
    "test:prod": "npm run build && PROD=true node_modules/.bin/mocha",
    "eject": "react-scripts eject",
    "electron": "ELECTRON_START_URL=http://localhost:3000 electron --inspect .",
    "postinstall": "cd ./node_modules/react-contextmenu; npm install; npm run build:dist && npm run build:es5 && npm run build:es6",
    "package": "npm run build && electron-packager ./build BabbleBuds --all --prune --overwrite --out ./out --icon build/public/icons/icon.icns"
  },
  "repository": "https://github.com/thepaperpilot/babble-buds",
  "keywords": [
    "puppet pals",
    "urealms"
  ],
  "author": "thepaperpilot",
  "license": "CC0-1.0",
  "devDependencies": {
    "@robertlong/react-ui-tree": "github:robertlong/react-ui-tree#dev",
    "babble.js": "~1.9.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "classnames": "^2.2.6",
    "electron-is-accelerator": "^0.1.2",
    "electron-packager": "^11.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.8.2",
    "flexlayout-react": "^0.2.5",
    "gif-engine-js": "^1.0.1",
    "js-search": "^1.4.2",
    "mocha": "^5.2.0",
    "pixi-viewport": "^3.1.0",
    "react": "^16.3.2",
    "react-animate-height": "^2.0.1",
    "react-autosize-textarea": "^3.0.3",
    "react-autosuggest": "^9.3.4",
    "react-contextmenu": "https://github.com/thepaperpilot/react-contextmenu/tarball/master",
    "react-custom-scroll": "git://github.com/thepaperpilot/react-custom-scroll.git#fix-click-scroll",
    "react-dnd": "^4.0.5",
    "react-dnd-html5-backend": "^4.0.5",
    "react-dom": "^16.3.2",
    "react-edit-inline": "^1.0.8",
    "react-pixi-fiber": "^0.4.9",
    "react-redux": "^5.0.7",
    "react-resize-detector": "^2.3.0",
    "react-scripts": "^3.0.1",
    "react-select": "^2.0.0-beta.6",
    "react-select-fast-filter-options": "^0.2.3",
    "react-virtualized-select": "^3.1.3",
    "react-window": "^1.8.1",
    "redux": "^4.0.0",
    "redux-undo": "^1.0.0-beta9-9-7",
    "spectron": "^5.0.0"
  },
  "dependencies": {
    "electron": "^3.0.0-beta.9",
    "electron-window-state": "~4.1.1",
    "fs-extra": "~5.0.0",
    "redux-logger": "^3.0.6",
    "semver": "^5.4.1",
    "socket.io": "~2.0.4",
    "socket.io-stream": "~0.9.1",
    "uuid": "^3.1.0"
  },
  "browserslist": [
    "last 1 electron version"
  ]
}
