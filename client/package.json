{
  "name": "babble-buds",
  "version": "0.8.2",
  "description": "An open source version of puppet pals",
  "main": "src/electron-starter.js",
  "homepage": "./",
  "scripts": {
    "start": "run-script-os",
    "start:nix": "BROWSER=none react-scripts start",
    "start:win32": "set BROWSER=none&&react-scripts start",
    "build": "run-script-os",
    "build:nix": "react-scripts build && ./build.sh",
    "build:win32": "react-scripts build && build.bat",
    "test": "node_modules/.bin/mocha",
    "test-watch": "node_modules/.bin/mocha -w --reporter min",
    "test-debug": "node_modules/.bin/mocha --inspect-brk",
    "electron": "run-script-os",
    "electron:nix": "ELECTRON_START_URL=http://localhost:3000 electron --inspect .",
    "electron:win32": "set ELECTRON_START_URL=http://localhost:3000&&electron --inspect .",
    "package": "npm run build && electron-packager ./build BabbleBuds --all --prune --overwrite --out ./out --icon build/public/icons/icon.icns"
  },
  "repository": "https://github.com/thepaperpilot/babble-buds",
  "keywords": [
    "puppet pals",
    "urealms"
  ],
  "author": "thepaperpilot",
  "license": "CC0-1.0",
  "devDependencies": {
    "babble.js": "~2.0.1",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "canvas": "^2.6.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-redux": "^1.0.2",
    "classnames": "^2.2.6",
    "electron-is-accelerator": "^0.1.2",
    "electron-packager": "^13.1.1",
    "electron-window-state": "~5.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "flexlayout-react": "^0.3.4",
    "gif-engine-js": "^1.0.1",
    "js-search": "^1.4.2",
    "jsdom": "^15.2.0",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.3",
    "pixi-viewport": "^3.23.1",
    "react": "^16.8.6",
    "react-animate-height": "^2.0.9",
    "react-autosize-textarea": "^7.0.0",
    "react-autosuggest": "^9.4.3",
    "react-contextmenu": "^2.11.0",
    "react-custom-scroll": "^4.1.5",
    "react-dnd": "^7.4.5",
    "react-dnd-html5-backend": "^7.4.4",
    "react-dom": "^16.8.6",
    "react-edit-inline": "^1.0.8",
    "react-pixi-fiber": "^0.8.0",
    "react-redux": "^5.0.7",
    "react-resize-detector": "^2.3.0",
    "react-scripts": "^3.0.1",
    "react-select": "^2.4.3",
    "react-select-fast-filter-options": "^0.2.3",
    "react-ui-tree": "github:thepaperpilot/react-ui-tree#master",
    "react-virtualized-select": "^3.1.3",
    "react-window": "^1.8.1",
    "redux": "^4.0.1",
    "redux-batched-subscribe": "^0.1.6",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^1.0.0-beta9-9-7",
    "run-script-os": "^1.0.7"
  },
  "dependencies": {
    "electron": "^5.0.1",
    "fs-extra": "~8.0.1",
    "pixi-tween": "^0.2.0",
    "redux-logger": "^3.0.6",
    "semver": "^6.0.0",
    "socket.io": "~2.2.0",
    "socket.io-stream": "~0.9.1",
    "uuid": "^3.3.2"
  },
  "browserslist": [
    "last 1 electron version"
  ]
}
